{
  "env": {
    "ENABLE_TOOL_SEARCH": "auto:0"
  },
  "permissions": {
    "allow": [
      "Bash(path:/tmp:*)"
    ]
  },
  "model": "sonnet",
  "hooks": {},
  "statusLine": {
    "type": "command",
    "command": "bash ~/.claude/statusline-command.sh"
  },
  "enabledPlugins": {
    "everything-claude-code@everything-claude-code": true,
    "superpowers@superpowers-dev": true
  },
  "autoUpdatesChannel": "latest"
}
