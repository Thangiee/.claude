{
  "model": "opus",
  "permissions": {
    "allow": [
      "Bash(path:/tmp:*)"
    ]
  },
  "hooks": {},
  "statusLine": {
    "type": "command",
    "command": "bash ~/.claude/statusline-command.sh"
  },
  "enabledPlugins": {
    "superpowers@claude-plugins-official": true,
    "everything-claude-code@everything-claude-code": true
  },
  "autoUpdatesChannel": "latest"
}
